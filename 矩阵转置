#include<iostream>
const int N = 10;
int a[N][N], b[N][N];
void main()
{
	int m, n, i, j;
	std::cout << "m="; std::cin >> m;
	std::cout << "n="; std::cin >> n;
	for(i=0;i<m;i++)
		for(j=0;j<n;j++)
		{
			std::cout << "a" << i + 1 << j + 1 << "="; std::cin >> a[i][j];
		}
	std::cout << "Matrix:\n";
	for(i=0;i<m;i++)
		for(j=0;j<n;j++)
		{
			std::cout << a[i][j] << "\t"; std::cout << "\n";
		}
	std::cout << "Transtpose:\n";
	for(i=0;i<n;i++)
		for(j=0;j<m;j++) b[i][j] = a[i][j];
	for (i = 0; i < n; i++)
		for (j = 0; j < m; j++)
		{
			std::cout << b[i][j] << "\t";
			std::cout << "\n";
		}
	system("pause");
}
